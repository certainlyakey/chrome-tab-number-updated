{
   "background": {
      "persistent": false,
      "scripts": [ "tabNumber.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Show tab number"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Show tab number",
   "key": "",
   "manifest_version": 2,
   "name": "Tab Number",
   "permissions": [ "tabs", "*://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1"
}
